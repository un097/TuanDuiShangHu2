<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lllll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/listtable_tv1"
        android:layout_width="120px"
        android:layout_height="60px"
        android:layout_weight="0.5"
        android:gravity="center_vertical"
        android:textColor="@color/black"
        android:textSize="@dimen/btnTextSizebody" />

    <TextView
        android:id="@+id/listtable_tv2"
        android:layout_width="140px"
        android:layout_height="60px"
        android:layout_weight="0.5"
        android:gravity="center_vertical"
        android:textColor="@color/black"
        android:textSize="@dimen/btnTextSizebody" />

    <TextView
        android:id="@+id/listtable_tv3"
        android:layout_width="140px"
        android:layout_height="60px"
        android:layout_weight="0.5"
        android:gravity="center_vertical"
        android:textColor="@color/black"
        android:textSize="@dimen/btnTextSizebody" />

    <TextView
        android:id="@+id/listtable_tv5"
        android:layout_width="40px"
        android:layout_height="60px"
        android:layout_weight="0.5"
        android:layout_marginLeft="25px"
        android:gravity="center_vertical"
        android:textColor="@color/black"
        android:textSize="@dimen/btnTextSizebody" />

    <TextView
        android:id="@+id/listtable_tv7"
        android:layout_width="40px"
        android:layout_height="60px"
        android:layout_weight="0.5"
        android:layout_marginLeft="30px"
        android:gravity="center_vertical"
        android:textColor="@color/black"
        android:textSize="@dimen/btnTextSizebody" />

    <techown.shanghu.biaoge.MyButton
        android:id="@+id/localCall"
        android:layout_width="0px"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3px"
        android:layout_weight="0.7"
        android:background="@drawable/myselector_bt"
        android:clickable="true"
        android:focusable="false"
        android:gravity="center"
        android:text="拨打本地"
        android:textColor="@color/black" />

    <techown.shanghu.biaoge.MyButton
        android:id="@+id/nonlocalCall"
        android:layout_width="0px"
        android:layout_height="wrap_content"
        android:layout_weight="0.7"
        android:background="@drawable/myselector_bt"
        android:clickable="true"
        android:focusable="false"
        android:gravity="center"
        android:text="拨打异地"
        android:textColor="@color/black" />

    <techown.shanghu.biaoge.MyButton
        android:id="@+id/shortcut_delete"
        android:layout_width="0px"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10px"
        android:layout_weight="0.7"
        android:background="@drawable/myselector_bt"
        android:clickable="true"
        android:focusable="false"
        android:gravity="center"
        android:text="修改"
        android:textColor="@color/black" />

</LinearLayout>