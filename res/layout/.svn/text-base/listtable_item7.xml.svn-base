<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >
    <TextView
    android:layout_width="200dip"
    android:layout_height="60dip" 
    android:textSize="@dimen/btnTextSizebody"
     android:layout_weight="1"
    android:id="@+id/listtable_tv1"
    android:textColor="@color/black"
    />
    <TextView
    android:layout_width="200dip"
    android:layout_height="60dip" 
    android:layout_weight="1"
	android:textSize="@dimen/btnTextSizebody"
	android:id="@+id/listtable_tv2"
	android:textColor="@color/black"
    />
     <TextView
    android:layout_width="200dip"
    android:layout_height="60dip" 
    android:layout_weight="1"
	android:textSize="@dimen/btnTextSizebody"
	android:id="@+id/listtable_tv3"
	android:textColor="@color/black"
    />

     <techown.shanghu.biaoge.MyButton
	  android:id="@+id/shortcut_zuihong"
	  android:gravity="center"
	  android:layout_width="80dp"
	  android:layout_height="wrap_content"
	  android:text="最红上线"	
	   android:textColor="@color/black"	 
	  android:background="@drawable/myselector_bt"
	  	  android:focusable="false"
	  android:clickable="true"
	
	 />
    <techown.shanghu.biaoge.MyButton
	  android:id="@+id/shortcut_youhui"
	  android:gravity="center"
	  android:layout_width="80dp"
	  android:layout_height="wrap_content"
	  android:text="特惠上线"	
	   android:textColor="@color/black"	 
	  android:background="@drawable/myselector_bt"
	  	  android:focusable="false"
	  android:clickable="true"
	
	 />
    
    </LinearLayout>