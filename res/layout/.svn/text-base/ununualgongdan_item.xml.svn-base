<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="horizontal" >

    <RadioButton android:id="@+id/appgd_mycheckbox"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:checked="false"
        android:button="@drawable/selector_radiobutton2"
        />
	<TextView android:id="@+id/appgd_starttime"
	    android:textSize="18dp"
	    android:textColor="@android:color/black"
	    android:layout_width="0dp"
	    android:gravity="center"
	    android:layout_weight="1.2"
	    android:layout_height="fill_parent"/>
	<TextView android:id="@+id/appgd_endtime"
	    android:textSize="18dp"
	    android:textColor="@android:color/black"
	    android:layout_width="0dp"
	    android:gravity="center"
	    android:layout_weight="1.2"
	    android:layout_height="fill_parent"/>
	<TextView android:id="@+id/appgd_needtime"
	    android:textSize="18dp"
	    android:layout_width="0dp"
	    android:textColor="@android:color/black"
	    android:gravity="center"
	    android:layout_weight="1.2"
	    android:layout_height="fill_parent"/>
	<TextView android:id="@+id/appgd_shname"
	    android:textSize="18dp"
	    android:layout_width="0dp"
	    android:gravity="center"
	    android:layout_weight="1.5"
	    android:textColor="@android:color/black"
	    android:paddingRight="2dp"
	    android:layout_height="fill_parent"/>
	<TextView android:id="@+id/appgd_shaddress"
	    android:textSize="18dp"
	    android:layout_width="0dp"
	    android:layout_weight="1.5"
	    android:gravity="center"
	    android:textColor="@android:color/black"
	    android:paddingRight="2dp"
	    android:layout_height="fill_parent"/>
	<Button android:id="@+id/appgd_btn_select"
	    android:textSize="18dp"
	    android:text="提交"
	    android:textColor="@android:color/black"
	    android:visibility="invisible"
	    android:background="@drawable/xiaoselector_bt"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"/>
</LinearLayout>