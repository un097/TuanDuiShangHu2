<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:panel="http://schemas.android.com/apk/res/lxp.panelex.view"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@drawable/b2" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="90dip"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="提报问题查询"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/black"
                android:textSize="@dimen/title" />
        </RelativeLayout>

        <LinearLayout 
            	android:layout_width="fill_parent"
            	android:layout_height="60dp"
            	android:layout_marginTop="3dp"
            	android:layout_marginLeft="50dp"
            	android:orientation="horizontal">
            
            <!--  <LinearLayout
	               android:layout_width="wrap_content"
	            	android:layout_height="fill_parent"
	            	android:id="@+id/statelayouttype"
	            	android:orientation="horizontal"
	            	android:layout_marginLeft="10dp"
	                > 
		            <TextView
		                android:layout_marginLeft="15dp"
		                android:layout_width="120dp"
		                 android:gravity="left"
		                android:paddingTop="14dp"
		                android:layout_height="fill_parent"
		                 android:textColor="@color/black"
		                android:textSize="@dimen/size" 
		                android:text="提报类型"
		                />
		            <TextView
		                android:id="@+id/statetexttype"
		                android:layout_width="200dp"
		                android:layout_height="wrap_content"
		                android:gravity="center"
		                android:background="@drawable/f2"
		                android:textColor="@color/gray1"
		                
		                android:textSize="@dimen/neirong"
		                android:layout_marginLeft="15dp">
		            </TextView>
		     </LinearLayout>
             -->
            
           <LinearLayout
               android:layout_width="wrap_content"
            	android:layout_height="fill_parent"
            	android:id="@+id/statelayout1"
            	android:clickable="true"
            	android:orientation="horizontal"
                >
               
                <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:paddingTop="15dp"
                android:layout_marginLeft="50dp"
                android:gravity="left"
                android:text="商户名称"
                android:textColor="@color/black"
                android:textSize="@dimen/size" 
                />
          	  <TextView
                android:id="@+id/statetext1"
                android:layout_width="240dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:background="@drawable/f2"
                android:textColor="@color/gray1"
                android:textSize="@dimen/neirong" 
                >
           		</TextView>
           </LinearLayout>
           
           <LinearLayout
               android:layout_width="wrap_content"
            	android:layout_height="fill_parent"
            	android:id="@+id/statelayout2"
            	android:layout_marginLeft="10dp"
            	android:orientation="horizontal"
                >   
	            <TextView
	                android:layout_marginLeft="15dp"
	                android:layout_width="120dp"
	                android:paddingTop="10dp"
	                android:layout_height="wrap_content"
	                android:textColor="@color/black"
	                android:gravity="left"
	                android:textSize="@dimen/size" 
	                android:text="提报开始日期"
	                />
	            <TextView
	                android:id="@+id/statetext2"
	                android:layout_width="200dp"
	                android:layout_height="fill_parent"
	                android:textColor="@color/gray1"
	                 android:gravity="center"
	                android:layout_marginTop="10dp"
	                android:background="@drawable/f2"
	                android:textSize="@dimen/neirong" 
	                android:layout_marginLeft="15dp">
	            </TextView>
	         </LinearLayout>   
	         
	        
        </LinearLayout>
        
        <LinearLayout 
            	android:layout_width="fill_parent"
            	android:layout_height="60dp"
            	android:layout_marginLeft="50dp"
            	android:orientation="horizontal">
            <LinearLayout
               android:layout_width="wrap_content"
            	android:layout_height="fill_parent"
            	android:id="@+id/statelayout3"
            	android:orientation="horizontal"
                >
		            <TextView
		                android:layout_width="120dp"
		                android:layout_height="wrap_content"
		                android:paddingTop="10dp"
		                android:text="提报结束日期"
		                 android:gravity="left"
		                 android:layout_marginLeft="50dp"
		                 android:textColor="@color/black"
		                android:textSize="@dimen/size" 
		                />
		            <TextView
		                android:id="@+id/statetext3"
		                android:layout_width="240dp"
		                android:layout_height="fill_parent"
		                android:layout_marginLeft="16dp"
		                 android:background="@drawable/f2"
		                 android:layout_marginTop="4dp"
		                  android:gravity="center"
		                 android:textColor="@color/gray1"
		                android:textSize="@dimen/neirong" 
		                >
			       	</TextView>
			 </LinearLayout>
			 <LinearLayout
	               android:layout_width="wrap_content"
	            	android:layout_height="fill_parent"
	            	android:id="@+id/statelayout4"
	            	android:orientation="horizontal"
	            	android:layout_marginLeft="10dp"
	                > 
		            <TextView
		                android:layout_marginLeft="15dp"
		                android:layout_width="120dp"
		                 android:gravity="left"
		                android:paddingTop="14dp"
		                android:layout_height="fill_parent"
		                 android:textColor="@color/black"
		                android:textSize="@dimen/size" 
		                android:text="当前状态"
		                />
		            <TextView
		                android:id="@+id/statetext4"
		                android:layout_width="200dp"
		                android:layout_height="wrap_content"
		                android:gravity="center"
		                android:background="@drawable/f2"
		                android:textColor="@color/gray1"
		                
		                android:textSize="@dimen/neirong"
		                android:layout_marginLeft="15dp">
		            </TextView>
		     </LinearLayout>
		     
            <Button android:id="@+id/statebutton1"
                 android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:background="@drawable/myselector_bt"
                android:textSize="@dimen/size" 
                android:text="查询"
                android:gravity="center"
                android:layout_marginTop="4dp"
                android:layout_marginLeft="15dp"
                />
            
        </LinearLayout>

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="30dip"
		    android:layout_marginLeft="10dip"
		    android:layout_marginRight="10dip"
		    android:orientation="horizontal"
		    android:layout_marginBottom="5dp"
		    android:paddingLeft="55dip"
		    android:paddingRight="40dip"
		    android:paddingTop="5dp" >

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1.2"
		        android:text="商户名称"
		        android:gravity="center"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1.2"
		        android:text="提出时间"
		        android:gravity="center"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="0.9"
		        android:text="问题描述"
		        android:gravity="center"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1.3"
		        android:text="审核结果"
		        android:gravity="center"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:gravity="center"
		        android:text="监控批注"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1.2"
		        android:text="故障一级类型"
		        android:gravity="center"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="0.9"
		        android:text="处理内容"
		        android:gravity="center"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />

		    <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1.2"
		        android:gravity="center"
		        android:text="结案时间"
		        android:textColor="@color/black"
		        android:textSize="@dimen/main" />
		</LinearLayout>

       	<ListView 
       	   		android:layout_marginLeft="52dip"
         		android:layout_marginRight="29dip"
                android:id="@+id/statelist"
              
                android:layout_width="match_parent"
                android:layout_height="400dip"
                android:orientation="vertical"
                >
       	</ListView>
            
	    <LinearLayout
	            android:layout_width="fill_parent"
	            android:layout_height="60dip"
	            android:gravity="center_vertical"
	            android:orientation="horizontal" >
	
	            <TextView
	                android:layout_width="320dp"
	                android:layout_height="wrap_content" />
	
	          
	            <TextView
	                android:layout_width="300dp"
	                android:layout_height="wrap_content" />
	
	            <Button
	                android:id="@+id/statebutton2"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:background="@drawable/myselector_bt"
	                android:text="返回"
	                android:textColor="@color/black"
	                android:textSize="@dimen/main" />
	        </LinearLayout>
    </LinearLayout>

</FrameLayout>