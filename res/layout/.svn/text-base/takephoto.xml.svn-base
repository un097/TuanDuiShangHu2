<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:panel="http://schemas.android.com/apk/res/lxp.panelex.view"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/b2" 
    >
    <RelativeLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="90dip"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="合约签订拍摄"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/black"
                android:textSize="@dimen/title" />
        </RelativeLayout>
    
	<LinearLayout
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:gravity="center"
	    android:orientation="vertical" >

	    <LinearLayout
	        android:layout_width="fill_parent"
	        android:layout_height="431dp"
	        android:layout_marginLeft="60dip"
	        android:layout_marginRight="60dip"
	        android:gravity="center"
	        android:orientation="horizontal" >

	        <LinearLayout
	            android:layout_width="300dp"
	            android:layout_height="fill_parent"
	            android:gravity="center"
	            android:orientation="vertical" >

	            <Button
	                android:id="@+id/takephoto1"
	                android:layout_width="300dp"
	                android:layout_height="wrap_content"
	                android:background="@drawable/myselector_changbt"
	                android:text="营业执照拍摄(必选)"
	                android:textColor="@color/black" />

	            <Button
	                android:id="@+id/takephoto2"
	                android:layout_width="300dp"
	                android:layout_height="wrap_content"
	                android:layout_marginTop="10dp"
	                android:background="@drawable/myselector_changbt"
	                android:text="盖章协议拍摄(必选)"
	                android:textColor="@color/black" />

	            <Button
	                android:id="@+id/takephoto3"
	                android:layout_width="300dp"
	                android:layout_height="wrap_content"
	                android:layout_marginTop="10dp"
	                android:background="@drawable/myselector_changbt"
	                android:text="授权书拍摄(可选)"
	                android:textColor="@color/black" />

	            <Button
	                android:id="@+id/takephoto4"
	                android:layout_width="300dp"
	                android:layout_height="wrap_content"
	                android:layout_marginTop="10dp"
	                android:background="@drawable/myselector_changbt"
	                android:text="其他附件拍照"
	                android:textColor="@color/black" />
	        </LinearLayout>

	        <LinearLayout
	            android:layout_width="300dp"
	            android:layout_height="fill_parent"
	            android:gravity="center"
	            android:paddingLeft="100dp" >

	            <ListView
	                android:id="@+id/imageList"
	                android:layout_width="200dp"
	                android:layout_height="384dp"
	                android:layout_alignParentRight="true"
	                android:layout_gravity="center"
	                android:divider="#00000000"
	                android:fadeScrollbars="true" >
	            </ListView>
	        </LinearLayout>
	    </LinearLayout>

	    <RelativeLayout
	        android:layout_width="755dp"
	        android:layout_height="wrap_content"
	         >

	        <Button
	            android:id="@+id/button7"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentLeft="true"
	            android:layout_centerVertical="true"
	            android:background="@drawable/myselector_bt"
	            android:text="返回"
	            android:textColor="@color/black"
	            android:textSize="@dimen/title" />

	        <Button
	            android:id="@+id/takephoto_before"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true"
	            android:background="@drawable/myselector_bt"
	            android:text="保存草稿箱"
	            android:textColor="@color/black"
	            android:textSize="@dimen/title" />

	        <Button
	            android:id="@+id/button6"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentTop="true"
	            android:layout_marginLeft="23dp"
	            android:layout_toRightOf="@+id/button7"
	            android:background="@drawable/myselector_bt"
	            android:text="提交"
	            android:textColor="@color/black"
	            android:textSize="@dimen/title" />

	    </RelativeLayout>
	</LinearLayout>

</FrameLayout>