<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="wrap_content"
    android:layout_height="60dip"
    android:id="@+id/bgsrc"
    >
    <TextView
    	android:layout_width="250dip"
    	android:layout_height="60dip" 
    	android:textSize="@dimen/btnTextSizebody"
     	android:layout_weight="1"
     	android:gravity="center"
     	android:paddingLeft="20dip"
    	android:id="@+id/listtable_tv1"
    	android:textColor="@color/black"
    />
    <TextView
    	android:layout_width="250dip"
    	android:layout_height="60dip" 
    	android:layout_weight="1"
    	android:gravity="center"
    	android:paddingLeft="40dip"
		android:textSize="@dimen/btnTextSizebody"
		android:id="@+id/listtable_tv2"
		android:textColor="@color/black"
    />
    <TextView
    	android:layout_width="250dip"
    	android:layout_height="60dip" 
		android:layout_weight="1"
		android:gravity="center"
		android:paddingLeft="30dip"
    	android:textSize="@dimen/btnTextSizebody"
    	android:id="@+id/listtable_tv3"
    	android:textColor="@color/black"
    />
    <LinearLayout 
        android:layout_width="250dip"
        android:layout_height="60dip"
        android:gravity="center"
        android:paddingLeft="65dip">
   <techown.shanghu.biaoge.MyButton
	  	android:id="@+id/shanghuxiadan"
	  	android:gravity="center"
	  	android:layout_width="130dp"
	  	android:layout_height="wrap_content"
	  	android:text="查看"	
	   	android:textColor="@color/black"	 
	  	android:background="@drawable/myselector_bt"
	  	android:focusable="false"
	 	android:clickable="true"
	 />
    </LinearLayout>
    </LinearLayout>