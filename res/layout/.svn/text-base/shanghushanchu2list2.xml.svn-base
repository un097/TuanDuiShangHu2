<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:id="@+id/bgsrc"
    >
    <TextView
    	android:layout_width="250dip"
    	android:layout_height="fill_parent" 
    	android:textSize="@dimen/btnTextSizebody"
     	android:layout_weight="1"
     	android:gravity="center"
    	android:id="@+id/listtable_tv1"
    	android:textColor="@color/black"
    />
    <TextView
    	android:layout_width="300dip"
    	android:layout_height="fill_parent" 
    	android:layout_weight="1"
    	android:gravity="center"
		android:textSize="@dimen/btnTextSizebody"
		android:id="@+id/listtable_tv2"
		android:textColor="@color/black"
    />
    <TextView
    	android:layout_width="300dip"
    	android:layout_height="fill_parent" 
		android:layout_weight="1"
		android:gravity="center"
    	android:textSize="@dimen/btnTextSizebody"
    	android:id="@+id/listtable_tv3"
    	android:textColor="@color/black"
    />
    <TextView
    	android:layout_width="270dip"
    	android:layout_height="fill_parent" 
		android:layout_weight="1"
		android:gravity="center"
    	android:textSize="@dimen/btnTextSizebody"
    	android:id="@+id/listtable_tv4"
    	android:textColor="@color/black"
    />
    <LinearLayout 
        android:layout_width="400dip"
        android:layout_height="fill_parent"
        android:gravity="center">
    <com.techown.test.uitl.MyButton
	  	android:id="@+id/shanchubtn"
	  	android:gravity="center"
	  	android:layout_width="wrap_content"
	  	android:layout_height="wrap_content"
	  	android:text="删除"	
	   	android:textColor="@color/black"	 
	  	android:background="@drawable/xiaoselector_bt"
	  	android:focusable="false"
	 	android:clickable="true"
	 />
    </LinearLayout>
    </LinearLayout>